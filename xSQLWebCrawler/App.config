<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <configSections>
    <section name="abot" type="Abot.Core.AbotConfigurationSectionHandler, Abot" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
